# Environment variables
GOOGLE_API_KEY=
OPENWEATHER_API_KEY=

# Social Media API Keys (for real data integration)
# Reddit API - Get from: https://www.reddit.com/prefs/apps (FREE)
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret

# News API - Get from: https://newsapi.org (FREE tier: 100 requests/day)
NEWS_API_KEY=your_news_api_key

# Telegram Bot (optional) - Get from: https://t.me/BotFather (FREE)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Email configuration
SENDER_EMAIL=your_email@gmail.com
EMAIL_PASSWORD=your_app_password_here
RECEIVER_EMAIL=recipient@gmail.com

# Monitoring cities (comma-separated)
CITIES=London,Karachi,New York,Tokyo

# Scheduling (minutes)
CHECK_INTERVAL=60

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Observability
ENABLE_TRACING=true
ENABLE_METRICS=true
METRICS_PORT=9090

# Memory and Session
SESSION_BACKEND=memory
MEMORY_RETENTION_DAYS=30

# Agent Configuration
MAX_RETRIES=3
TIMEOUT_SECONDS=30
ENABLE_PARALLEL_AGENTS=true

# Evaluation
ENABLE_EVALUATION=false
EVALUATION_THRESHOLD=0.8
